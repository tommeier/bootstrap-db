<h1>Listing time_data</h1>

<table>
  <thead>
    <tr>
      <th>Subject</th>
      <th>Time value</th>
      <th>Timestamp value</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @time_data.each do |time_datum| %>
      <tr>
        <td><%= time_datum.subject %></td>
        <td><%= time_datum.time_value %></td>
        <td><%= time_datum.timestamp_value %></td>
        <td><%= link_to 'Show', time_datum %></td>
        <td><%= link_to 'Edit', edit_time_datum_path(time_datum) %></td>
        <td><%= link_to 'Destroy', time_datum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Time datum', new_time_datum_path %>
